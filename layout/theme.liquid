<body class="gradient {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}">
<a class="skip-to-content-link button visually-hidden" href="#MainContent">
    {{ "accessibility.skip_to_text" | t }}
</a>

{%- render 'cart-drawer' -%}
{% section 'header' %}
{% render 'breadcrumbs' %}
<main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
    {{ content_for_layout }}
</main>

{% section 'footer' %}

<ul hidden>
    <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
</ul>

<script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`
      }

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`
      }
</script>

{%- if settings.predictive_search_enabled -%}
    <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

{% comment %}
  Villatheme Code
{% endcomment %}
<script src="{{ 'jquery.js' | asset_url }}"></script>
<script src="{{ 'jquery.magnific-popup.js' | asset_url }}"></script>
<script src="{{ 'vtcustom.js' | asset_url }}"></script>
<script src="{{ 'vtcountdown.js' | asset_url }}"></script>
{%- if settings.cart_type == 'notification' -%}
    <script src="{{ 'vtcartdropdown.js' | asset_url }}"></script>
{%- endif -%}
{% if template == 'index' or template contains 'page' %}
    <script src="{{ 'product-form.js' | asset_url }}"></script>
{%- endif -%}
<script src="{{ 'vtquickview.js' | asset_url }}" async></script>
<script>
      window.lazySizesConfig = window.lazySizesConfig || {};
      window.lazySizesConfig.expand = 10;
</script>
<script src="{{ 'lazysizes.js' | asset_url }}"></script>

{% if settings.vt_enablewish %}
    {{ 'wishlist.js' | asset_url | script_tag }}
    <script>
        $(".vtwishremove").on('click', function() {
          $(this).addClass("vtloadtowish vtwishadding");
          setTimeout(function() {
            $(".vtwishremove").removeClass("vtloadtowish");
          }, 1000);
          setTimeout(function() {
            $(".vtwishadding").removeClass("vtwishadding");
          }, 2000);
        });
        (function() {
          Wishlist.init();
        }());
    </script>
{% endif %}

{% if request.locale.iso_code == 'ar' %}
    <link rel="stylesheet" href="{{ 'rtl.css' | asset_url }}" type="text/css">
{% endif %}

</body>
</html>